<?xml version="1.0" encoding="utf-8"?>
<resources>
    <style name="Transparent" parent="@android:style/Theme.DeviceDefault.Light">
        <item name="android:windowBackground">@android:color/transparent</item>
        <item name="android:windowNoTitle">true</item>
        <item name="android:windowIsFloating">true</item>
    </style>
    <style name="NfcChnEnablePopupTheme" parent="@android:style/Theme.DeviceDefault.Light.Dialog">
        <item name="android:windowBackground">@android:color/transparent</item>
        <item name="android:windowNoTitle">true</item>
    </style>
    <style name="Dialog" parent="@android:style/Theme.DeviceDefault.Light.Dialog">
        <item name="android:windowBackground">@android:color/transparent</item>
        <item name="android:windowNoTitle">true</item>
        <item name="android:windowIsFloating">true</item>
    </style>
    <style name="NfcFullTableDialog" parent="@android:style/Theme.DeviceDefault.Light.Dialog">
        <item name="android:windowBackground">@android:drawable/expander_open_mtrl_alpha</item>
    </style>
    <style name="NoTitleBar" parent="@android:style/Theme.DeviceDefault.Light.Dialog">
        <item name="android:windowNoTitle">true</item>
        <item name="android:windowContentOverlay">@null</item>
    </style>
</resources>
